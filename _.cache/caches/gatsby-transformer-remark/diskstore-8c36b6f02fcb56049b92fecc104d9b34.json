osition":{"start":{"line":26,"column":1,"offset":1407},"end":{"line":26,"column":163,"offset":1569}}},{"type":"heading","depth":2,"children":[{"type":"text","value":"Faithful Representation","position":{"start":{"line":28,"column":4,"offset":1574},"end":{"line":28,"column":27,"offset":1597}}}],"position":{"start":{"line":28,"column":1,"offset":1571},"end":{"line":28,"column":27,"offset":1597}}},{"type":"paragraph","children":[{"type":"text","value":"Faithful representation means that the actual effects of the transactions shall be properly accounted for and reported in the financial statements. The words and numbers must match what really happened in the transaction. The ingredients of faithful representation are completeness, neutrality and free from error.","position":{"start":{"line":30,"column":1,"offset":1599},"end":{"line":30,"column":315,"offset":1913}}}],"position":{"start":{"line":30,"column":1,"offset":1599},"end":{"line":30,"column":315,"offset":1913}}},{"type":"heading","depth":2,"children":[{"type":"text","value":"Enhancing Qualitative Characteristics","position":{"start":{"line":32,"column":4,"offset":1918},"end":{"line":32,"column":41,"offset":1955}}}],"position":{"start":{"line":32,"column":1,"offset":1915},"end":{"line":32,"column":41,"offset":1955}}},{"type":"heading","depth":3,"children":[{"type":"text","value":"Verifiability","position":{"start":{"line":34,"column":5,"offset":1961},"end":{"line":34,"column":18,"offset":1974}}}],"position":{"start":{"line":34,"column":1,"offset":1957},"end":{"line":34,"column":18,"offset":1974}}},{"type":"paragraph","children":[{"type":"text","value":"Verifiability implies consensus between the different knowledgeable and independent users of financial information. Such information must be supported by sufficient evidence to follow the principle of objectivity.","position":{"start":{"line":35,"column":1,"offset":1975},"end":{"line":35,"column":214,"offset":2188}}}],"position":{"start":{"line":35,"column":1,"offset